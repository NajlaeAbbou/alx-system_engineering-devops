#!/usr/bin/env bash

# Loop through files and directories in the current directory
for item in *; do
    # Check if the item is a file or directory and not hidden
    if [ -f "$item" ] || [ -d "$item" ]; then
        # Extract the part after the first dash
        name_part=$(echo "$item" | cut -d '-' -f 2-)
        
        # Display the extracted name part
        echo "$name_part"
    fi
done
